                <a href="#"><i class="fa fa-google-plus"></i></a>
                <a href="#"><i class="fa fa-youtube"></i></a>
              </div>
            </div>
            <div class="col-md-6 col-sm-12 col-xs-12">
              <div class="aa-footer-left mobilemargin">
                <p>All rights reserved<a rel="nofollow" href="http://www.markups.io/">@MaaKripaAutoDeals</a></p>
               </div>
              
              </div>
            </div>            
          </div>
        </div>
      </div>
      </div>
    </div>
      
  </footer>
  <!-- / Footer -->


 
