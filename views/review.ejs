<!-- Advance Search -->
<section id="aa-advance-search">
  <div class="container">
    <form action="/home/search" method="post">
      <div class="aa-advance-search-area">
        <div class="form">
         <div class="aa-advance-search-top">
            <div class="row flex">
              <div class="col-md-4">
                <div class="aa-single-advance-search">
                  <select name="budget">
                   <option value="0" selected>select your budget</option>
                    <option value="300000">under 3,00,000</option>
                    <option value="500000">under 5,00,000</option>
                    <option value="700000">under 7,00,000</option>
                    <option value="1000000">under 10,00,000</option>
                    <option value="1200000">under 12,00,000</option>
                    <option value="1500000">under 15,00,000</option>
                    <option value="2000000">under 20,00,000</option>
                    <option value="2000001">20,00,000 +</option>
                  </select>
                </div>
              </div>
              <div class="col-md-2">
                <div class="aa-single-advance-search">
                  <select name="cartype">
                   <option value="all" selected>All Cars</option>
                    <option value="kia">Kia</option>
                    <option value="tata">Tata</option>
                    <option value="hyundai">Hyundai</option>
                    <option value="toyota">Toyota</option>
                    <option value="mahindra">Mahindra</option>
                    <option value="ford">Ford</option>
                    <option value="suzuki">Suzuki</option>
                    <option value="other">Others</option>
                  </select>
                </div>
              </div>
              <div class="col-md-2">
                <div class="aa-single-advance-search">
                  <input class="aa-search-btn" type="submit" value="Search">
                </div>
              </div>
            </div>
          </div>
        
         </div>
        </div>
      </div>
    </form>
  </div>
</section>
<!-- / Advance Search -->



  <!-- Client Testimonial -->
  <section id="aa-client-testimonial">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-client-testimonial-area">
            <div class="aa-title">
              <h2>What Client Say</h2>
              <span></span>
              <p>Hear from our satisfied clientsâ€”your trusted partner in finding the perfect property!</p>
            </div>
            <!-- testimonial content -->
            <div class="aa-testimonial-content">
              <!-- testimonial slider -->
              <ul class="aa-testimonial-slider">
                <% for(review of rev){ %>
                <li>
                  <div class="aa-testimonial-single">
                    <div class="aa-testimonial-img">
                      <img src=<%= review.image %> alt="testimonial img">
                    </div>
                    <div class="aa-testimonial-info">
                      <p><%= review.content %></p>
                    </div>
                    <div class="aa-testimonial-bio">
                      <p><%= review.name %></p>
                      <span><%= review.profession %></span>
                    </div>
                  </div>
                </li>
                <% } %>
              </ul>
              <div class="ahref">
                <a href="/home/review/addreview">add review</a>
              </div>
            </div>
          </div>
        </div>
      </div> 
